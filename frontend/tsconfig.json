{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "sourceMap": true,
    "listFiles": true,
    "allowJs": true,
    "jsx": "react",
    "esModuleInterop": true,
    "noImplicitAny": false,
    "noImplicitThis": true,
    "strictNullChecks": true
  },
  "include": ["src/js/*"],
  "exclude": ["node_modules"]
}
